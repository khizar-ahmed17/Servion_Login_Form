    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>JavaServlet</servlet-name>
    <url-pattern>/JavaServlet</url-pattern> 
    <url-pattern>/login</url-pattern>
    
  </servlet-mapping>
